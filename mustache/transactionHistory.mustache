  <table id='transactionHistoryTable'>
    <thead>
      <tr>
        <th >date</th>
        <th>account</th>
        <th>amount</th>
        <th>category</th>
        <th>subCategory</th>
        <th>month</th>
        <th>year</th>
        <th>notes</th>
      </tr>
    </thead>
    <tbody>
{{#.}}
  <tr id="{{_id}}">
    <td > {{date}}</td>
    <td> {{account}}</td>
    <td> {{amount}}</td>
    <td> {{category}}</td>
    <td> {{subCategory}}</td>
    <td> {{month}}</td>
    <td> {{year}}</td>
    <td> {{notes}}</td>
    <td><button class="deleteProductBtn">x</button></td>
  </tr>
    {{/.}}
    </tbody>
  </table>